{
  "contentTypes": {
    "bulb-brand": {
      "kind": "collectionType",
      "collectionName": "bulb_brands",
      "info": {
        "singularName": "bulb-brand",
        "pluralName": "bulb-brands",
        "displayName": "BulbBrand"
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "name": {
          "type": "string",
          "required": true
        },
        "slug": {
          "type": "uid",
          "targetField": "name"
        },
        "isActive": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "bulb-type": {
      "kind": "collectionType",
      "collectionName": "bulb_types",
      "info": {
        "singularName": "bulb-type",
        "pluralName": "bulb-types",
        "displayName": "BulbType"
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "name": {
          "type": "string",
          "required": true
        },
        "code": {
          "type": "string",
          "required": true
        },
        "description": {
          "type": "text"
        },
        "category": {
          "type": "enumeration",
          "enum": ["headlight", "fog_light", "signal_light", "interior", "exterior"],
          "default": "headlight"
        },
        "isActive": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "bulb-compatibility": {
      "kind": "collectionType",
      "collectionName": "bulb_compatibilities",
      "info": {
        "singularName": "bulb-compatibility",
        "pluralName": "bulb-compatibilities",
        "displayName": "BulbCompatibility"
      },
      "options": {
        "draftAndPublish": true
      },
      "attributes": {
        "vehicleBrand": {
          "type": "string",
          "required": true
        },
        "vehicleModel": {
          "type": "string",
          "required": true
        },
        "vehicleYear": {
          "type": "string"
        },
        "bulbType": {
          "type": "relation",
          "relation": "manyToOne",
          "target": "api::bulb-type.bulb-type"
        },
        "position": {
          "type": "enumeration",
          "enum": ["headlight", "fog_light", "signal_light", "interior", "exterior"],
          "default": "headlight"
        },
        "partNumber": {
          "type": "string"
        },
        "isActive": {
          "type": "boolean",
          "default": true
        }
      }
    }
  }
}
