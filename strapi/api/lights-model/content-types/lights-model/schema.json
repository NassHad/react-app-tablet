{
  "kind": "collectionType",
  "collectionName": "lights_models",
  "info": {
    "singularName": "lights-model",
    "pluralName": "lights-models",
    "displayName": "LightsModel"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "constructionYearStart": {
      "type": "string"
    },
    "constructionYearEnd": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "lightsBrand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lights-brand.lights-brand",
      "inversedBy": "models"
    },
    "positions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::lights-position.lights-position",
      "mappedBy": "lightsModel"
    }
  }
}
